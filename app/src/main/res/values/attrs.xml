<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Keyboard">
        <attr name="keyWidth" format="dimension|fraction" />
        <attr name="keyHeight" format="dimension|fraction" />
        <attr name="horizontalGap" format="dimension|fraction" />
        <attr name="verticalGap" format="dimension|fraction" />
    </declare-styleable>
    <declare-styleable name="Keyboard_Key">
        <attr name="codes" format="integer|string" />
        <attr name="iconPreview" format="color|string|flags" />
        <attr name="popupCharacters" format="string" />
        <attr name="popupKeyboard" format="color" />
        <attr name="isRepeatable" format="dimension|fraction" />
        <attr name="isModifier" format="boolean" />
        <attr name="isSticky" format="boolean" />
        <attr name="keyEdgeFlags" format="flags" />
        <attr name="keyIcon" format="color|string|flags" />
        <attr name="keyLabel" format="string" />
        <attr name="keyOutputText" format="string" />
    </declare-styleable>

</resources>